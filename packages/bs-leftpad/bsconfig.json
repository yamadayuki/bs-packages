{
  "name": "bs-leftpad",
  "version": "0.1.0",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        {
          "dir": "__tests__",
          "type": "dev"
        }
      ]
    }
  ],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "package-specs": {
    "module": "commonjs",
    "in-source": false
  },
  "suffix": ".bs.js",
  "warnings": {
    "errors": "+101"
  },
  "refmt": 3
}
